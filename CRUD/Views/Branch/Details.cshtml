            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.br_brgy)
            </dd>
    
            <dt>
                @Html.Raw("Branch City")

            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.br_city)
            </dd>
    
            <dt>
                @Html.Raw("Branch Manager")

            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.br_mngr)

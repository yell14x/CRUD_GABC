@model CRUD.Models.Branch

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Details</title>
</head>
<body>
    <div>
        <h4>Branch</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
            @Html.Raw("Branch code")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.BR_Code)
            </dd>
    
            <dt>
                @Html.Raw("Branch Name")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.br_name)
            </dd>
    
            <dt>
                @Html.Raw("Branch Address")
            </dt>
    
            <dd>
                @Html.DisplayFor(model => model.br_address)
